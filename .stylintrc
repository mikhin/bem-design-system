{
	"reporter": "stylint-json-reporter",
	"blocks": "never",
	"brackets": "never",
	"colons": "never",
	"colors": false,
	"commaSpace": "always",
	"commentSpace": "always",
	"cssLiteral": "never",
	"depthLimit": 5,
	"duplicates": true,
	"efficient": "always",
	"extendPref": "@extends",
	"globalDupe": false,
	"indentPref": false,
	"leadingZero": "never",
	"maxErrors": false,
	"maxWarnings": false,
	"namingConvention": "BEM",
	"namingConventionStrict": false,
	"none": "always",
	"noImportant": true,
	"parenSpace": "never",
	"placeholders": false,
	"prefixVarsWithDollar": true,
	"quotePref": "single",
	"semicolons": "never",
	"stackedProperties": "never",
	"universal": "never",
	"valid": true,
	"mixins": [
		"--word-width-s",
		"--word-width-m",
		"--word-width-l",
		"--space_xxxs",
		"--space_xxs",
		"--space_xs",
		"--space_s",
		"--space_m",
		"--space_l",
		"--space_xl",
		"--space_xxl",
		"--space_xxxl",
		"--space_xxxxl",
		"--space_xxxxxl",
		"--text_height_s",
		"--text_height_m",
		"--text_height_l",
		"--text_height_xl",
		"--text_height_xxl",
		"--text_line_height_s",
		"--text_line_height_m",
		"--text_line_height_l",
		"--text_line_height_xl",
		"--text_line_height_xxl",
		"--image",
		"--text_color_link",
		"--text_color_primary",
		"--color_default",
		"--color_accent",
		"--color_border",
		",size",
		"margin-x",
		"margin-y",
		"padding-x",
		"padding-y",
		"sprite",
		"retinaSprite"
	],
	"zeroUnits": "never",
	"zIndexNormalize": false
}
